{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-cloud-2024"
		},
		"ls_ADLS_Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADLS_Storage'"
		},
		"ls_SQLTargetDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_SQLTargetDB'"
		},
		"ls_ADLS_Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlscloudstorage2024.dfs.core.windows.net/"
		},
		"ls_SQLSerevr_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KeyVaultCloud.vault.azure.net/"
		},
		"ls_SQLTargetDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf-dev-practice.database.windows.net"
		},
		"ls_SQLTargetDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TargetDB"
		},
		"ls_SQLTargetDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "surendra"
		},
		"ls_Source_TumblingTrigger_SQLSerevr_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLSerevr-adf-dev-practice"
		},
		"trigger1_properties_pipeline_parameters_StartTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"trigger1_properties_pipeline_parameters_EndTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-cloud-2024_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ADLS_Storage_accountKey": {
						"value": "[parameters('ls_ADLS_Storage_accountKey')]"
					},
					"ls_SQLTargetDB_password": {
						"value": "[parameters('ls_SQLTargetDB_password')]"
					},
					"ls_ADLS_Storage_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLS_Storage_properties_typeProperties_url')]"
					},
					"ls_SQLSerevr_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_SQLSerevr_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_SQLTargetDB_properties_typeProperties_server": {
						"value": "[parameters('ls_SQLTargetDB_properties_typeProperties_server')]"
					},
					"ls_SQLTargetDB_properties_typeProperties_database": {
						"value": "[parameters('ls_SQLTargetDB_properties_typeProperties_database')]"
					},
					"ls_SQLTargetDB_properties_typeProperties_userName": {
						"value": "[parameters('ls_SQLTargetDB_properties_typeProperties_userName')]"
					},
					"ls_Source_TumblingTrigger_SQLSerevr_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_Source_TumblingTrigger_SQLSerevr_properties_typeProperties_connectionString_secretName')]"
					},
					"trigger1_properties_pipeline_parameters_StartTime": {
						"value": "[parameters('trigger1_properties_pipeline_parameters_StartTime')]"
					},
					"trigger1_properties_pipeline_parameters_EndTime": {
						"value": "[parameters('trigger1_properties_pipeline_parameters_EndTime')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}